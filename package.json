{
  "name": "growth-ts-api",
  "version": "1.3.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "yarn tsc",
    "start:prod": "yarn && yarn db:migrate && node build/index.js",
    "start": "nodemon ./src/index.ts",
    "db:migrate": "yarn run knex migrate:latest --knexfile src/database/knexfile.ts",
    "db:rollback": "yarn run knex migrate:rollback --knexfile src/database/knexfile.ts"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.15",
    "prettier": "2.8.4",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-joi-validation": "^5.0.1",
    "helmet": "^6.0.1",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^1.0.7",
    "lodash": "^4.17.21",
    "mixpanel": "^0.16.0",
    "moment": "^2.29.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3"
  },
  "packageManager": "yarn@3.4.1"
}
